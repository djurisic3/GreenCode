{
  "name": "greencode",
  "displayName": "GreenCode",
  "description": "Elevate your coding efficiency with GreenCode â€“ smartly optimizing for reduced energy consumption.",
  "publisher": "GreenCode",
  "icon": "images/greencode_vscode_logo.png",
  "version": "0.1.7",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Linters",
    "Snippets"
  ],
  "keywords": [
    "eco-friendly",
    "sustainability",
    "green coding",
    "code optimization",
    "performance",
    "energy saving",
    "efficient coding",
    "SQL",
    "PL/SQL",
    "MySQL"
  ],
  "activationEvents": [
    "onCommand:markDirtyCode",
    "onCommand:deactivateMarkDirtyCode"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/djurisic3/GreenCode.git"
  },
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "greencode.markDirtyCode",
        "key": "shift+ctrl+alt+f"
      },
      {
        "command": "greencode.deactivateMarkDirtyCode",
        "key": "shift+ctrl+alt+d"
      },
      {
        "command": "greencode.cleanMarkedCode",
        "key": "ctrl+space"
      },
      {
        "command": "greencode.promptLoginData"
      },
      {
        "command": "greencode.activateAI",
        "key": "ctrl+alt+i"
      }
    ],
    "commands": [
      {
        "command": "greencode.activateAI",
        "title": "GreenCode: Use AI to analyze code"
      },
      {
        "command": "greencode.findSqlQueries",
        "title": "GreenCode: Analyze database for optimization suggestions"
      },
      {
        "command": "greencode.cleanMarkedCode",
        "title": "GreenCode: Replace dirty code with green code"
      },
      {
        "command": "greencode.markDirtyCode",
        "title": "GreenCode: Activate the dirt seeker"
      },
      {
        "command": "greencode.deactivateMarkDirtyCode",
        "title": "GreenCode: Deactivate the dirt seeker"
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/djurisic3/GreenCode/issues"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.65",
    "@types/oracledb": "^5.2.5",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "esbuild": "^0.19.8",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "mysql2": "^3.9.4",
    "node-sql-parser": "^5.0.0",
    "openai": "^3.2.1",
    "oracledb": "^5.5.0"
  }
}
